{
  "name": "@cocso-ui/react-icons",
  "version": "0.0.2-beta.8",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cocso/cocso-ui.git",
    "directory": "packages/react-icons"
  },
  "scripts": {
    "build": "rm -rf lib && bun run build.js && bun x tsc",
    "dev": "bun run build.js --watch"
  },
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./brand": {
      "types": "./lib/components/brand/index.d.ts",
      "import": "./lib/components/brand/index.js",
      "require": "./lib/components/brand/index.cjs"
    },
    "./semantic": {
      "types": "./lib/components/semantic/index.d.ts",
      "import": "./lib/components/semantic/index.js",
      "require": "./lib/components/semantic/index.cjs"
    },
    "./graphic": {
      "types": "./lib/components/graphic/index.d.ts",
      "import": "./lib/components/graphic/index.js",
      "require": "./lib/components/graphic/index.cjs"
    },
    "./brand/*": {
      "types": "./lib/components/brand/*.d.ts",
      "import": "./lib/components/brand/*.js",
      "require": "./lib/components/brand/*.cjs"
    },
    "./semantic/*": {
      "types": "./lib/components/semantic/*.d.ts",
      "import": "./lib/components/semantic/*.js",
      "require": "./lib/components/semantic/*.cjs"
    },
    "./graphic/*": {
      "types": "./lib/components/graphic/*.d.ts",
      "import": "./lib/components/graphic/*.js",
      "require": "./lib/components/graphic/*.cjs"
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "esbuild": "^0.25.8",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
