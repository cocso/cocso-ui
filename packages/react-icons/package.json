{
  "name": "@cocso-ui/react-icons",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cocso/cocso-ui.git",
    "directory": "packages/react-icons"
  },
  "scripts": {
    "build": "rm -rf lib && bun run build.js && bun x tsc",
    "dev": "bun run build.js --watch",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --fix"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "esbuild": "^0.25.8",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
